<% provide(:title, 'Create a House') %>
<h1>Create a House</h1>
<div class="row">
	<%= form_for(@house) do |f| %>
		<%= f.label :name %>
		<%= f.text_field :name %>

  	<%= f.label :address %>
		<%= f.text_area :address, size: "60x12" %>

		<%= f.submit "Create a House", class: "btn" %>
	<% end %>
</div>
